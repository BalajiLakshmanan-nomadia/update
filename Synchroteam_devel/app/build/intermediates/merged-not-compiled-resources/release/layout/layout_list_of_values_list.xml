<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fontify="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:background="@color/semiTransparent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="@dimen/widthNewSigantureFactureLinearLayout"
        android:layout_height="@dimen/heightMultiDialogContainer"
        android:orientation="vertical" >

        <com.synchroteam.TypefaceLibrary.TextView
            android:id="@+id/categoryNameTv"
            style="@style/styleDomainDialogHeaderLogin"
            android:gravity="center"
            android:text="@string/liste"
            android:padding="@dimen/paddingLoginDomainDialogTextView"
            fontify:font_name="@string/fontName_hevelicaLight" />

        <ListView
            android:id="@+id/listOfValuesList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white" >
        </ListView>
    </LinearLayout>

</LinearLayout>